<%= form_for @employee do |f| %> 
<%= f.label :name, "Enter your name" %>
<%= f.text_field :name %> <br>
<%= f.label :title, "Job Title" %>
<%= f.text_field :title %>
<%= f.label :company, "Pick your company" %>
<%= f.select :company_id, Company.all.map{|company| [company.name, company.id]} %>
<%= f.submit %>
<% end %>